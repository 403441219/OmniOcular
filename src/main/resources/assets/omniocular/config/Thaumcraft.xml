<oo>
	<init>
	function up(word){
		return word.substring(0,1).toUpperCase()+word.substring(1)
	}
	</init>
    <tileentity id="TileResearchTable">
        <line displayname="Bonus Aspects">
				return up(nbt['bonusAspects'][0]['tag'])
        </line>
    </tileentity>
	<tileentity id="TileArcaneWorkbench">
        <line displayname="Aer">
			if(nbt['Inventory'][0]['tag']['aer']!=undefined)
				return (nbt['Inventory'][0]['tag']['aer']/100).toFixed(0)
        </line>
		<line displayname="Aqua">
			if(nbt['Inventory'][0]['tag']['aqua']!=undefined)
				return (nbt['Inventory'][0]['tag']['aqua']/100).toFixed(0)
        </line>
		<line displayname="Ignis">
			if(nbt['Inventory'][0]['tag']['ignis']!=undefined)
				return (nbt['Inventory'][0]['tag']['ignis']/100).toFixed(0)
        </line>
		<line displayname="Ordo">
			if(nbt['Inventory'][0]['tag']['ordo']!=undefined)
				return (nbt['Inventory'][0]['tag']['ordo']/100).toFixed(0)
        </line>
		<line displayname="Perditio">
			if(nbt['Inventory'][0]['tag']['perditio']!=undefined)
				return (nbt['Inventory'][0]['tag']['perditio']/100).toFixed(0)
        </line>
		<line displayname="Terra">
			if(nbt['Inventory'][0]['tag']['terra']!=undefined)
				return (nbt['Inventory'][0]['tag']['terra']/100).toFixed(0)
        </line>
    </tileentity>
	<tileentity id="TileDeconstructionTable">
        <line displayname="Aspect">
				return up(nbt['Aspect'])
        </line>
    </tileentity>
	<tileentity id="TilePedestal">
		<line>
			name(nbt['Items'][0])
		</line>
	</tileentity>
    <tileentity id="TileInfusionStone">
        <line displayname="Instability">
			if(nbt['active']!=0)
				return nbt['instability'].toFixed(0)
        </line>
    </tileentity>
    <tileentity id="TileAlchemyFurnace">
        <line displayname="Vis Amount">
            return nbt['Vis']+GRAY+" / "+WHITE+"50"
        </line>
    </tileentity>
    <tileentity id="TileSiphon">
        <line displayname="Vis Amount">
            return nbt['amount']+GRAY+" / "+WHITE+"32"
        </line>
    </tileentity>
    <tileentity id="TileCrucible">
        <line>
            return WHITE+nbt['Amount']+GRAY+" / "+WHITE+"1000 mB"
        </line>
    </tileentity>
    <tileentity id="TileSpa">
		<line displayname="Bath Salts">
            return nbt['Items']['0']['Count']
        </line>
        <line>
			if(nbt['FluidName']!=undefined)
				return WHITE+nbt['Amount']+GRAY+" / "+WHITE+"5000 mB"
        </line>
    </tileentity>
    <tileentity id="TileJarBrain">
        <line displayname="XP">
			if(nbt['XP']!=0)
				return nbt['XP']
        </line>
    </tileentity>
	<tileentity id="TileFocalManipulator">
		<line>
			name(nbt['ItemsSynced'][0])
		</line>
	</tileentity>
	<tileentity id="TileMirror">
        <line displayname="Linked with">
			if(nbt['linked']!=0)
				return nbt['linkX']+", "+nbt['linkY']+", "+nbt['linkZ']
        </line>
    </tileentity>
	<tileentity id="TileMirrorEssentia">
        <line displayname="Linked with">
			if(nbt['linked']!=0)
				return nbt['linkX']+", "+nbt['linkY']+", "+nbt['linkZ']
        </line>
    </tileentity>
	<tileentity id="TileNode">
		<line displayname="Node Type">
			switch(nbt['type']){
				case 0:return "Normal";
					break;
				case 1:return "Unstable";
					break;
				case 2:return "Sinister";
					break;
				case 3:return "Tainted";
					break;
				case 4:return "Hungry";
					break;
				case 5:return "Pure";
					break;}
	</line>
	</tileentity>
	<entity id="Thaumcraft.Golem">
		<line displayname="Core">
			switch(nbt['Core']){
				case 0:return "Fill";
					break;
				case 1:return "Empty";
					break;
				case 2:return "Gather";
					break;
				case 3:return "Harvest";
					break;
				case 4:return "Guard";
					break;
				case 5:return "Decanting";
					break;
				case 6:return "Alchemy";
					break;
				case 7:return "Chop";
					break;
				case 8:return "Use";
					break;
				case 9:return "Butcher";
					break;
				case 10:return "Sorting";
					break;
				case 11:return "Fishing";
					break;}
	</line>
	</entity>
</oo>
