<oo>
    <tileentity id="Rotary Macerator">
        <line displayname="ic2.generic.text.Energy">
            return nbt['energy']+GRAY+" / "+WHITE+"10112 EU"
        </line>
        <line displayname="Speed">
            return nbt['speed']+GRAY+" / "+WHITE+"10000 RPM"
        </line>
    </tileentity>
    <tileentity id="Singularity Compressor">
        <line displayname="ic2.generic.text.Energy">
            return nbt['energy']+GRAY+" / "+WHITE+"10112 EU"
        </line>
        <line displayname="Pressure">
            return nbt['speed']*9+GRAY+" / "+WHITE+"90000 Gibbl"
        </line>
    </tileentity>
	<tileentity id="Centrifuge Extractor">
        <line displayname="ic2.generic.text.Energy">
