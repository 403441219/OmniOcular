<oo>
    <tileentity id="witchery:altar">
        <line>
            return nbt['Power'].toFixed(0)+" / "+nbt['MaxPower']*nbt['PowerScale']+" (x"+nbt['PowerScale']+")"
        </line>
    </tileentity>
	<tileentity id="witchery:cauldron">
        <line>
            return nbt['Amount']+" / 3000 mB"
        </line>
    </tileentity>
	<tileentity id="witchery:leechchest">
        <line>
            return "Owner : "+nbt['WITCPlayers'][0]['Player']
        </line>
    </tileentity>
</oo>
