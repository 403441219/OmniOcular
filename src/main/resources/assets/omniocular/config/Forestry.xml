<oo>
    <tileentity id="forestry.Engine.*">
        <line displayname="Energy Stored">
            return nbt['EngineStored']
        </line>
        <line displayname="Heat">
            return nbt['EngineHeat']/10+20
        </line>
        <line displayname="Burn Time Left">
            return (nbt['EngineBurnTime']/20).toFixed(0)+' Seconds'
        </line>
        <line displayname="Fuel Tank">
            return nbt['FuelSlot']['Amount'].toFixed(0)+'mB'
        </line>
        <line displayname="Heating Tank">
            return nbt['HeatingSlot']['Amount'].toFixed(0)+'mB'
        </line>
    </tileentity>
    <tileentity id="forestry.Raintank">
        <line displayname="Water Amount">
            return nbt['ResourceTank']['Amount'].toFixed(0)+'mB'
        </line>
    </tileentity>
    <tileentity id="forestry.Generator.*">
        <line displayname="Energy Stored">
            return nbt['IC2BasicSource']['energy'].toFixed(0)+'EU'
        </line>
        <line displayname="Biomass Amount">
            return nbt['ResourceTank']['Amount'].toFixed(0)+'mB'
        </line>
    </tileentity>
