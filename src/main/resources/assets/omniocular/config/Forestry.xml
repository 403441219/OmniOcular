<oo>
    <tileentity id="forestry.Analyzer">
        <line displayname="Honey">
            return nbt['tanks'][0]['Amount']+" mB"
        </line>
    </tileentity>
    <tileentity id="forestry.EngineTin">
        <line displayname="Energy Stored">
            return nbt['EnergyManager']['EnergyStorage']['Energy']+" RF"
        </line>
        <line displayname="Energy Stored">
            return nbt['IC2BasicSink']['energy']+" EU"
        </line>
        <line displayname="Heat">
            return nbt['EngineHeat']/10+20
        </line>
    </tileentity>
    <tileentity id="forestry.EngineCopper">
        <line displayname="Energy Stored">
            return nbt['EnergyManager']['EnergyStorage']['Energy']+" RF"
        </line>
        <line displayname="Burn time">
            return (nbt['EngineBurnTime']/20).toFixed(0)+" / "+(nbt['EngineTotalTime']/20).toFixed(0)+" Seconds"
        </line>
        <line displayname="Heat">
            return nbt['EngineHeat']/10+20
        </line>
    </tileentity>
