<oo>
    <tileentity id="blockKillerJoeTileEntity">
        <line displayname="hud.msg.EnderIO.Experience">
            return nbt['experienceTotal']
        </line>
    </tileentity>
    <tileentity id="blockZombieGeneratorTileEntity">
        <line displayname="enderio.killerJoe.fuelTank">
            return nbt['fuelTank']['Amount']+'/2000 mB'
        </line>
    </tileentity>
    <tileentity id="blockPoweredSpawnerTileEntity">
        <line displayname="hud.msg.EnderIO.SpawnChance">
            return nbt['currentTask']['chance']
        </line>
    </tileentity>
    <tileentity id="blockCombustionGeneratorTileEntity">
        <line displayname="enderio.combustionGenerator.coolantTank">
            return nbt['coolantTank']['Amount']+'/5000 mB'
        </line>
        <line displayname="enderio.combustionGenerator.fuelTank">
            return nbt['fuelTank']['Amount']+'/5000 mB'
        </line>
    </tileentity>
    <tileentity id="blockVatTileEntity">
        <line displayname="enderio.vat.inputTank">
            return nbt['inputTank']['Amount']+'/8000 mB'
        </line>
        <line displayname="enderio.vat.outputTank">
            return nbt['outputTank']['Amount']+'/8000 mB'
        </line>
    </tileentity>
    <tileentity id="blockSoulBinderTileEntity">
        <line displayname="hud.msg.EnderIO.Experience">
            return nbt['experienceTotal']
        </line>
    </tileentity>
</oo>