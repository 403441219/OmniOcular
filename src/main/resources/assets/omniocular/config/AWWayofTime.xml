<oo>
    <tileentity id="containerAltar">
        <line displayname="Life Essence">
            return nbt['Amount']
        </line>
        <line displayname="Capacity">
            return nbt['capacity']
        </line>
        <line displayname="LP proceed">
            return nbt['progress']
        </line>
        <line displayname="LP required">
            return nbt['liquidRequired']
        </line>
        <line displayname="Tier">
            return nbt['upgradeLevel']
        </line>
        <line displayname="Capacity *">
            return nbt['capacityMultiplier']
        </line>
        <line displayname="Speed *">
            return nbt['consumptionMultiplier']
        </line>
        <line displayname="Dislocation *">
            return nbt['dislocationMultiplier']
        </line>
        <line displayname="Efficiency *">
            return nbt['efficiencyMultiplier']
        </line>
        <line displayname="Orb capacity *">
            return nbt['orbCapacityMultiplier']
        </line>
        <line displayname="Sacrifice *">
            return nbt['sacrificeEfficiencyMultiplier']
        </line>
        <line displayname="Self-Sacrifice *">
            return nbt['selfSacrificeEfficiencyMultiplier']
        </line>
    </tileentity>
    <tileentity id="containerMasterStone">
        <line displayname="Ritual ID">
            return nbt['currentRitualString']
        </line>
        <line displayname="Ritual Owner">
            return nbt['owner']
        </line>
    </tileentity>
    <tileentity id="containerWritingTable">
        <line displayname="LP required(*100)">
            return nbt['amountUsed']
        </line>
        <line displayname="Progress(%)">
            return nbt['progress']
        </line>
    </tileentity>
</oo>