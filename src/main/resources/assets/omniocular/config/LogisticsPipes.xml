<oo>
    <tileentity id="logisticspipes.*">
        <line displayname="Power Level">
            return nbt['powerLevel']+' / 2000000 LP'
        </line>
    </tileentity>
    <tileentity id="logisticspipes.blocks.powertile.LogisticsBCPowerProviderTileEntity">
        <line displayname="Energy">
            return nbt['internalStorage'].toFixed(0)+' / 1000000 MJ'
        </line>
    </tileentity>
    <tileentity id="logisticspipes.blocks.powertile.LogisticsRFPowerProviderTileEntity.disable">
        <line displayname="Energy">
            return nbt['internalStorage'].toFixed(0)+' / 10000000 MJ'
        </line>
    </tileentity>
    <tileentity id="logisticspipes.blocks.powertile.LogisticsIC2PowerProviderTileEntity">
        <line displayname="Energy">
            return nbt['internalStorage'].toFixed(0)+' / 40000000 EU'
        </line>
    </tileentity>
</oo>